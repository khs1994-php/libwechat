{
  "name": "khs1994/wechat",
  "description": "A PHP library for the WeChat ",
  "keywords": [
    "sdk",
    "api"
  ],
  "homepage": "https://github.com/khs1994-php/libwechat",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "khs1994",
      "email": "khs1994@khs1994.com",
      "homepage": "https://khs1994.com"
    }
  ],
  "support": {
    "issues": "https://github.com/khs1994-php/libwechat/issues"
  },
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "doctrine/cache": "^1.6",
    "khs1994/curl": "dev-master",
    "khs1994/tencent-ai": "^18.03",
    "pimple/pimple": "~3.0"
  },
  "require-dev": {
    "ext-PDO": "*",
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "platform": {
      "php": "7.1.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "suggest": {
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "WeChat\\": "src/WeChat/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WeChat\\Tests\\": "tests/WeChat/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "18.06-dev"
    }
  }
}
